  .site-menu { height: 0px; }
  .main-menu.main-menu-open .site-menu { height: auto; }
  .menu-flex-container { display: none; }
  .menu-item-has-children::after { border-bottom: 2px solid rgb(4, 38, 61); border-left: 2px solid rgb(4, 38, 61); border-image: initial; border-radius: 2px; border-right: 0px; border-top: 0px; content: ""; display: block; height: 0.625em; margin-top: -0.4375em; pointer-events: none; position: absolute; top: 22px; transform: rotate(-45deg); transform-origin: center center; width: 0.625em; right: 30px; transition: all 0.15s ease-out 0s; }
  .menu-item-has-children.active::after { transform: rotate(-225deg); }
}

@media screen and (max-width: 1200px) {
  .yarpp-thumbnails-horizontal .yarpp-thumbnail { width: 330px !important; }
  .yarpp-thumbnail > img, .yarpp-thumbnail-default { width: 300px !important; }
  .yarpp-thumbnails-horizontal .yarpp-thumbnail-title { width: 300px !important; }
  .is-sticky { top: 140px; }
}

.is-invisible { opacity: 0; }

.table-striped { font-family: "Open Sans", sans-serif; color: rgb(20, 40, 46); }

.table-striped tr th, .table-striped tr td { padding: 25px 10px; }

.table-striped tr th { font-size: 16px; font-weight: 700; text-align: left; padding-left: 40px; }

.table-striped tr:nth-child(2n+1) { background-color: transparent !important; }

.table-striped tr:nth-child(2n) { background-color: rgb(250, 250, 252); }

.table-striped tr td { font-size: 14px; }

.table-striped tr td:first-child { text-align: left; width: 20%; padding-left: 25px; }

.table-striped tr td:not(:first-child) { vertical-align: top; }

.table-striped ul { margin: 0px 0px 10px; }

.table-striped ul > li { padding-bottom: 10px; }

.modal-backdrop.in { opacity: 0; }

.modal.ebook-download-popup { text-align: center; background: rgba(53, 75, 83, 0.5); backdrop-filter: blur(5px); z-index: 9999; overflow-y: hidden; }

.modal.ebook-download-popup .modal-body { padding: 38px 44px; }

.modal.ebook-download-popup .modal-content { background: rgb(241, 244, 250); border-radius: 12px; box-shadow: rgba(0, 0, 0, 0.05) 0px 8px 24px, rgba(0, 0, 0, 0.05) 0px 4px 8px, rgba(0, 0, 0, 0.05) 0px 2px 2px; }

.modal.ebook-download-popup h4 { font-size: 32px; font-weight: 600; line-height: 1.5; margin-bottom: 40px; margin-top: 0px; color: rgb(4, 38, 61); }

.modal.ebook-download-popup .close { position: absolute; right: 15px; top: 15px; width: 16px; height: 16px; opacity: 1; }

.modal.ebook-download-popup .close::before, .modal.ebook-download-popup .close::after { position: absolute; top: 0px; content: " "; height: 16px; width: 1px; background-color: rgb(136, 161, 170); }

.modal.ebook-download-popup .close::before { transform: rotate(45deg); }

.modal.ebook-download-popup .close::after { transform: rotate(-45deg); }

.modal.ebook-download-popup input { border-radius: 4px; width: 240px; padding: 10px 8px; border: 1px solid rgb(136, 161, 170); }

.modal.ebook-download-popup .error-message { color: rgb(240, 82, 82); font-size: 12px; margin: 0px; line-height: 1.3; }

.modal.ebook-download-popup .form-control.has-error { border-color: rgb(240, 82, 82); }

.modal.ebook-download-popup .cta-group { display: inline-block; text-align: center; margin-top: 12px; }

.modal.ebook-download-popup .btn-primary { background: rgb(14, 118, 188); border-color: rgb(14, 118, 188); border-radius: 6px; padding: 7px 14px; font-size: 14px; display: block; min-width: 170px; }

.modal.ebook-download-popup .link { font-weight: 500; font-size: 12px; line-height: 16px; color: rgb(136, 161, 170); display: block; margin-top: 5px; }

.modal.ebook-download-popup .modal-dialog { display: inline-block; text-align: left; vertical-align: middle; }

@media screen and (min-width: 768px) {
  .modal.ebook-download-popup::before { display: inline-block; vertical-align: middle; content: " "; height: 100%; }
  .modal.ebook-download-popup .modal-dialog { width: 672px; }
  .modal.ebook-download-popup .modal-body { min-height: 410px; }
  .modal.ebook-download-popup h4 { padding-right: 60px; }
  .modal.ebook-download-popup .modal-content { background: url("./images/ebook-download-popup-bg.png") right bottom no-repeat rgb(241, 244, 250); }
}
